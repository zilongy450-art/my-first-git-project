					"script": {
						"exec": [
							"// 正例测试：添加地址成功\r",
							"pm.test(\"返回状态码应为 201 Created\", function () {\r",
							"    pm.response.to.have.status(201); // 检查状态码为 201\r",
							"});\r",
							"\r",
							"pm.test(\"JSON 中 success 字段为 true\", function () {\r",
							"    var json = pm.response.json();\r",
							"    pm.expect(json.success).to.be.true;\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"JSON 中 message 字段为 地址添加成功\", function () {\r",
							"    var json = pm.response.json();\r",
							"    pm.expect(json.message).to.eql(\"地址添加成功\");\r",
							"});\r",
							"\r",
							"\r",
							"pm.test(\"JSON 中 address_id 字段存在\", function () {\r",
							"    var json = pm.response.json();\r",
							"    pm.expect(json).to.have.property(\"address_id\");\r",
							"});\r",
							"\r",
							"\r",
							"// 解析响应 JSON\r",
							"let jsonData = pm.response.json();\r",
							"// 把 address_id 存到变量中（环境变量）\r",
							"pm.environment.set(\"address_id1\", jsonData.address_id);"
